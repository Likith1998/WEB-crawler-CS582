<script type="text/javascript">
function ctf(t,c){
  if(t==''){return true;}var o=new Object();
  if(typeof o[t]=='undefined') return true;
  var oa=o[t].split('|');for(var i=0;i<oa.length;i++){if(typeof o[oa[i]]!='undefined'){return true;}}
  return (o[t].indexOf('|'+c+'|')>-1);
}
</script>
